
<% if(success && success.length){ %>
    <div class="alert alert-success alert-dismissible fade show max-wid_1 m-auto mb-2" role="alert">
        <span><%= success %></span>
        <button type="button" class="btn-close" data-bs-dismiss="alert" aria-label="Close"></button>
    </div>
<%}%>

<% if(error && error.length){ %>
    <div class="alert alert-danger alert-dismissible fade show max-wid_1 m-auto mb-2" role="alert">
        <span><%= error %></span>
        <button type="button" class="btn-close" data-bs-dismiss="alert" aria-label="Close"></button>
    </div>
<%}%>